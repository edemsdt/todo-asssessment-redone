<for |todo| of=state.allData>
  <li key=todo._id>
      <div class="form-check"> <label class="form-check-label"> <input class=['checkbox', todo.checked && 'completed'] type="checkbox" id=todo._id checked=todo.checked on-change('handleCheckboxChange')> ${todo.task} <i class="input-helper"></i></label> </div> <i class="remove mdi mdi-close-circle-outline delete-icon" id=todo._id on-click('handleDelete')></i>
  </li>
</for>